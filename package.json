{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.158",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-postcss-sass": "^1.0.16",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-source-filesystem": "^1.5.18",
    "gatsby-transformer-remark": "^1.7.31",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "postcss": "postcss src/layouts/ps-styles/**/*.postcss --output src/layouts/styles/styles.css -w",
    "dev-ps": "run-p postcss & npm run develop"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.17",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.10.2"
  }
